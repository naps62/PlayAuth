- content_for :head do
	= stylesheet_link_tag 'application'
	= javascript_include_tag 'application'

!!!
%head
	= yield :head

%body
	- flash.each do |name, msg|
		= content_tag :div, msg, :id => "frash_#{name}" if msg.is_a?(String)

	%table
		%tr
			%td.nav= render 'layouts/nav'
			%td.content= yield